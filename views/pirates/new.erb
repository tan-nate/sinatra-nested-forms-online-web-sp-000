  <label for="ship_name_2">Ship 2 name:</label><input type="text" name="pirate[ships][][name]" id="ship_name_2"><br>
  <label for="ship_type_2">Ship 2 type:</label><input type="text" name="pirate[ships][][type]" id="ship_type_2"><br>
