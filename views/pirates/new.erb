  <label for="pirate[weight]">Pirate weight:</label><input type="text" name="pirate[weight]" id="pirate[weight]"><br>
  <label for="pirate[height]">Pirate height:</label><input type="text" name="pirate[height]" id="pirate[height]"><br>
  <label for="ship_name_1">Ship 1 name:</label><input type="text" name="pirate[ships][][name]" id="ship_name_1"><br>
  <label for="ship_type_1">Ship 1 type:</label><input type="text" name="pirate[ships][][type]" id="ship_type_1"><br>
